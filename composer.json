{
  "name": "chenhaitry/laravel-command",
  "description": "A sample laravel command",
  "type": "library",
  "homepage": "https://github.com/chenhaitry/laravel-command",
  "license": "MIT",
  "authors": [
    {
      "name": "ChenhaiTry",
      "email": "chenhaitry64@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1 || ^8.0",
    "laravel/framework": "^5.4 || ^6.0 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7.0 || ^8.0",
    "graham-campbell/testbench": "^4.0 || ^5.0",
    "mockery/mockery": "^1.4"
  },
  "support": {
    "issues": "https://github.com/chenhaitry/laravel-command/issues",
    "source": "https://github.com/chenhaitry/laravel-command"
  },
  "autoload": {
    "psr-4": {
      "Chenhaitry\\LaravelCommand\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": ["tests"]
  },
  "repositories": [
    {"type": "github", "url": "https://github.com/chenhaitry/laravel-command"},
    {"packagist.org": false}
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Chenhaitry\\LaravelCommand\\Provider\\LaravelCommandServiceProvider"
      ]
    }
  }
}
